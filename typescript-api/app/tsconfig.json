{
  "compilerOptions": {
    "sourceMap": true,
//    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "target": "ES2020",
    "module": "ES2022",
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "noUnusedParameters": false,
    "noUnusedLocals": false,
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
